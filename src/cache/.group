#!/usr/bin/env CLI_TOOL=cli bash-cli-part

cli::cache::help() {
    cat << EOF
Command
    ${CLI_COMMAND[@]}
EOF
}

cli::cache::main() {
    : # special loader case; export nothing
}

cli::cache::self_test() {
    cli cache path --self-test
    cli cache put --self-test
    cli cache test --self-test
}
