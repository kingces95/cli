#!/usr/bin/env CLI_TOOL=cli bash-cli-part

cli::args::help() {
    cat << EOF
Command
    ${CLI_COMMAND[@]}

Description

EOF
}

cli::args::main() {
    : # special loader case; export nothing
}

cli::args::self_test() {
    cli args check --self-test
    cli args parse --self-test
    cli args resolve --self-test
    cli args tokenize --self-test
    cli args verify --self-test
}
