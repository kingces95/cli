#!/usr/bin/env CLI_NAME=cli bash-cli-part

cli::core::type::help() {
    cat << EOF
Command
    ${CLI_COMMAND[@]}
EOF
}

cli::core::type::main() {
    :
}

cli::core::type::self_test() {
    cli core type emit --self-test
    cli core type get --self-test
    cli core type get-info --self-test
    cli core type resolve --self-test
    cli core type to-bash --self-test
    cli core type unmodify --self-test
    cli core type unmodified --self-test
}
