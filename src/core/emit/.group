#!/usr/bin/env CLI_TOOL=cli bash-cli-part

cli::core::emit::help() {
    cat << EOF
Command
    ${CLI_COMMAND[@]}

Description
EOF
}

cli::core::emit::main() {
    :
}

cli::core::emit::self_test() {
    cli core emit variable --self-test
    cli core emit global --self-test
    cli core emit scope --self-test
}
